#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int checkPassword(char s[20])
{
    char *password = "jeiks";
    /*
    password = (char*) malloc(6);
    password[0] = 'j';
    password[1] = 'e';
    password[2] = 'i';
    password[3] = 'k';
    password[4] = 's';
    password[5] = '\0';
    */
    if ( strcmp(password, s) == 0 )
        return 1;
    else
        return 0;
}
